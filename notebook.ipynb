    "import networkx as nx\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "def generate_random_graph(n, p):\n",
    "    G = nx.erdos_renyi_graph(n, p)\n",
    "    return G\n",
    "\n",
    "\n",
    "\n",
    "G = generate_random_graph(10, 0.1)\n",
    "nx.draw(G, with_labels=True)\n",
    "plt.show()\n"
   ]
  }
